{
  "@@locale": "en",

{%- for module in modules %}
  "{{ module.camel_name }}Singular": "{{ module.translations.singular.en }}",
  "{{ module.camel_name }}Gender": "{{ module.gender }}",
  "@{{ module.camel_name }}Singular": {},
  "{{ module.camel_name }}Plural": "{{ module.translations.plural.en }}",
  "@{{ module.camel_name }}Plural": {},
  "{{ module.camel_name }}List": "{{ module.translations.plural.en }}",
  "@{{ module.camel_name }}List": {},
  "{{ module.camel_name }}ListSubtitle": "Browse and manage all {{ module.translations.plural.en.lower() }}",
  "@{{ module.camel_name }}ListSubtitle": {},
  "{{ module.camel_name }}CreateSubtitle": "Add a new {{ module.translations.singular.en.lower() }}",
  "@{{ module.camel_name }}CreateSubtitle": {},
{%- if module.export %}
  "{{ module.camel_name }}ExportSubtitle": "Export {{ module.translations.plural.en.lower() }} data",
  "@{{ module.camel_name }}ExportSubtitle": {},
{%- endif %}

{%- if module.translations and module.translations.fields %}
{%- for field_name, field_translations in module.translations.fields.items() %}
{%- if field_translations.en %}
  "{{ module.camel_name }}{{ field_name.capitalize() }}Label": "{{ field_translations.en.label }}",
  "@{{ module.camel_name }}{{ field_name.capitalize() }}Label": {},
  "{{ module.camel_name }}{{ field_name.capitalize() }}Hint": "{{ field_translations.en.hint }}",
  "@{{ module.camel_name }}{{ field_name.capitalize() }}Hint": {},
{%- endif %}
{%- endfor %}
{%- endif %}

{%- endfor %}

