{
  "@@locale": "en",

{%- for module in modules %}
  "{{ module.snake_name }}Singular": "{{ module.translations.singular.en }}",
  "@{{ module.snake_name }}Singular": {},
  "{{ module.snake_name }}Plural": "{{ module.translations.plural.en }}",
  "@{{ module.snake_name }}Plural": {},
  "{{ module.snake_name }}List": "{{ module.translations.plural.en }}",
  "@{{ module.snake_name }}List": {},
  "{{ module.snake_name }}ListSubtitle": "Browse and manage all {{ module.translations.plural.en.lower() }}",
  "@{{ module.snake_name }}ListSubtitle": {},
  "{{ module.snake_name }}CreateSubtitle": "Add a new {{ module.translations.singular.en.lower() }}",
  "@{{ module.snake_name }}CreateSubtitle": {},
{%- if module.export %}
  "{{ module.snake_name }}ExportSubtitle": "Export {{ module.translations.plural.en.lower() }} data",
  "@{{ module.snake_name }}ExportSubtitle": {},
{%- endif %}

{%- if module.translations and module.translations.fields %}
{%- for field_name, field_translations in module.translations.fields.items() %}
{%- if field_translations.en %}
  "{{ module.snake_name }}{{ field_name.capitalize() }}Label": "{{ field_translations.en.label }}",
  "@{{ module.snake_name }}{{ field_name.capitalize() }}Label": {},
  "{{ module.snake_name }}{{ field_name.capitalize() }}Hint": "{{ field_translations.en.hint }}",
  "@{{ module.snake_name }}{{ field_name.capitalize() }}Hint": {},
{%- endif %}
{%- endfor %}
{%- endif %}

{%- endfor %}

